{
  "name": "keylani",
  "version": "0.0.1",
  "description": "A js key binding library",
  "scripts": {
    "postinstall": "cypress verify",
    "test:setup": "nyc --reporter=html --reporter=text-summary mocha --recursive ./src/test/**/*.test.js",
    "test": "nodemon --exec 'npm run test:setup'",
	"lint": "eslint .",
	"build": "webpack --progress",
	"compile": "export NODE_ENV=production && npm run lint && npm run test:setup && npm run build",
	"dev": "export NODE_ENV=development && npm run lint && npm run build",
	"cy": "cypress run --spec src/e2e/*.spec.js"
  },
  "author": "Simao Nziaka",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "cypress": "^3.4.1",
    "dotenv": "^8.0.0",
    "eslint": "^6.1.0",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "sinon": "^7.4.1",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6"
  }
}
