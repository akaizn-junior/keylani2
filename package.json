{
  "name": "keylani",
  "version": "0.0.1",
  "description": "A js key binding library",
  "scripts": {
    "postinstall": "cypress verify",
    "test:setup": "nyc mocha --recursive ./src/test/**/*.test.js",
    "test": "nodemon --exec 'npm run test:setup'",
    "lint": "eslint .",
    "build": "webpack --progress",
    "compile": "export NODE_ENV=production && npm run lint && npm run test:setup && npm run build",
    "dev": "export NODE_ENV=development && npm run lint && npm run build",
    "cy": "http-server & cypress run --spec src/e2e/*.spec.js",
    "docs": "jsdoc src/keylani.interface.js -c jsdoc/conf.json --verbose && cp logo.png docs"
  },
  "author": "Simao Nziaka",
  "license": "MIT",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 10,
    "statements": 10,
    "functions": 10,
    "branches": 0,
    "reporter": [
      "html",
      "text-summary"
    ],
    "exclude": [
      "**/*.test.js"
    ],
    "extension": [
      ".js"
    ],
    "cache": true,
    "all": false
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "cypress": "^3.4.1",
    "docdash": "^1.1.1",
    "dotenv": "^8.0.0",
    "eslint": "^6.1.0",
    "http-server": "^0.11.1",
    "jsdoc": "^3.6.3",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "sinon": "^7.4.1",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6"
  }
}
